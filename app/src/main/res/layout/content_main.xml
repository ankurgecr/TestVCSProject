<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".MainActivity"
    tools:showIn="@layout/activity_main">

    <Button
        android:id="@+id/btn_test_ankur"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/action_test_ankur_project" />

    <Button
        android:id="@+id/btn_test_govind"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/action_test_govind_project" />

    <Button
        android:id="@+id/btn_test_sapna"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/action_test_sapna_project" />

    <Button
        android:id="@+id/btn_test_himanshu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/action_test_himanshu_project" />

</LinearLayout>